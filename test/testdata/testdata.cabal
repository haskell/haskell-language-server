name:                testdata
version:             0.1.0.0
cabal-version:       >=2.0
build-type: Simple

executable codeactionrename
  build-depends:       base
  main-is:             CodeActionRename.hs
  default-language:    Haskell2010

executable hover
  build-depends:       base
  main-is:             Hover.hs
  default-language:    Haskell2010

executable symbols
  build-depends:       base
  main-is:             Symbols.hs
  default-language:    Haskell2010

executable hlintpragma
  build-depends:       base
  main-is:             HlintPragma.hs
  default-language:    Haskell2010

executable functests
  build-depends:       base
  main-is:             FuncTest.hs
  default-language:    Haskell2010

executable evens
  build-depends:       base
  main-is:             Evens.hs
  hs-source-dirs:      liquid
  default-language:    Haskell2010

executable filewithwarning
  build-depends:       base
  main-is:             FileWithWarning.hs
  default-language:    Haskell2010
